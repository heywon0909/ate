{
  "name": "eslint-plugin-ate",
  "version": "0.0.8",
  "description": "This is eslint-plugin for ate-Eight",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "ate-Eight",
  "main": "./dist/lib/index.js",
  "exports": "./dist/lib/index.js",
  "scripts": {
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
